import java.util.Scanner;

public class futebol {
    public static void main(String[] args) {
        try (Scanner sc = new Scanner(System.in)) {
        
        System.out.println("Digite seu nome:");
        String letras= sc.nextLine();

        System.out.println("Digite sua idade:");
        Integer idad= sc.nextInt();

            if (idad<=10){
                System.out.println("Sua categoria é infantil," + letras);
            }
            else{
                if (idad<=15){
                    System.out.println("Sua categoria é juvenil," + letras);
                }
                else{
                    if (idad<=20){
                    System.out.println("Sua categoria é junior," + letras);
                    }    
                }
                
            }
            if(idad>20){
                        System.out.println("Sua categoria é profissional," + letras);
                    }
                    else{
                        System.out.println("");
                    }
                }

    }}
